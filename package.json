{
  "name": "playwright_template",
  "version": "1.0.0",
  "description": "This is a template project for Playwright with TypeScript",
  "main": "index.js",
  "scripts": {
    "tests": "playwright test -c configs/playwright.config.ts",
    "tests:headed": "playwright test -c configs/playwright.config.ts --headed",
    "tests:config:chromium": "playwright test -c configs/playwright.chromium.config.ts",
    "tests:list": "playwright test -c configs/playwright.config.ts --list",
    "tests:project": "playwright test -c configs/playwright.config.ts --project firefox",
    "tests:spec": "playwright test -c configs/playwright.config.ts moreExamples",
    "report:html:show": "playwright show-report reports/html"
  },
  "keywords": [
    "playwright",
    "typescript",
    "javascript",
    "template"
  ],
  "author": "Andrii Kravchenko (https://github.com/AndreyCar)",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.51.1",
    "@types/node": "^22.7.9",
    "dotenv": "^16.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
